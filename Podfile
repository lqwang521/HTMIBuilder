target 'HTMITemplate' do
    # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
    use_frameworks!
    
    # Pods for SQTemplate
    pod 'AFNetworking', '~> 3.0'
    pod 'SDWebImage', '~>3.8'
    pod 'SVProgressHUD', '~> 2.1.2'
    # 路由组件化解耦
    pod 'JLRoutes', '~> 2.0.5'
    pod 'FMDB'
    pod 'Aspects'
    pod 'ReactiveObjC', '~> 3.0.0'
end
